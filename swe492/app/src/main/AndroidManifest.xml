<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="swe.omm.omm">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Omm"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".EditCardActivity"></activity>
        <activity android:name=".MeetingModeActivity"></activity>
        <activity android:name=".ShareCardActivity"></activity>
        <activity android:name=".ViewCardActivity"></activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".SaveActivity"></activity>
        <activity
            android:name=".RegisterActivity"
            android:label="Register"></activity>
    </application>

</manifest>